{
  "name": "Technical_Assessment",
  "version": "1.0.0",
  "description": "This project is to perform automation testing for Katalan Ecommerce website",
  "main": "index.js",
  "scripts": {
    "testRun_Headed": "npx cypress run --spec cypress/e2e/features/*.feature --headed --browser chrome",
    "testRun_Headless": "npx cypress run --spec cypress/e2e/features/*.feature --headless --browser chrome"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^12.17.4",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-mochawesome-reporter": "^3.5.1",
    "cypress-xpath": "^2.0.1"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "step_definitions": "cypress/support/step_definitions/"
  }
}
